    <tr><td><%= link_to_function "About Us", :onclick => "window.open(this.href);return false;", :href => "http://webroar.in/aboutus"%></td></tr>
    <tr><td><%= link_to_function "Contact Us", :onclick => "window.open(this.href);return false;", :href => "http://webroar.in/contactus"%><br><br></td></tr>
  </table>
</div>
<div id='contact_us'>
    <%= render :partial => 'feedback_partial' %>
</div>



