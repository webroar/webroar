<div id = "page_name"><%= image_tag("title_contact_us_28_06_2011.gif", :border => '0')%></div>
<br/><br/>
<div id="contact_us_left_panel">
  <table>
    <tr><td><%= link_to_remote "Feedback",:update => 'contact_us' ,:url => {:controller => 'admin',:action => 'contact_us',:form_name => 'feedback'}%></td></tr>
